n          </v-layout>\r\n\r\n\r\n          <!--new item button -->\r\n              <div>\r\n                  <v-dialog v-model=\"dialog\" max-width=\"500px\">\r\n                  <template v-slot:activator=\"{ on }\">\r\n                      <v-layout justify-start row>\r\n                          <v-flex xs2>\r\n                              <v-btn color=\"blue darken-1\" dark class=\"mb-2 \" v-on=\"on\">New Item</v-btn>\r\n                          </v-flex>\r\n                      </v-layout>\r\n                  </template>\r\n                  <v-card>\r\n                    <v-card-title>\r\n                        <h1> New Item </h1>\r\n                    </v-card-title>\r\n\r\n\r\n                    <v-card-text>\r\n                      <v-container grid-list-md>\r\n                        <v-layout wrap>\r\n                          <v-flex xs12 sm6 md4>\r\n                            <v-select\r\n                              v-model=\"newMarketplace\"\r\n                              label=\"MarketPlace\"\r\n                              :items=\"marketplaces\"\r\n                              >\r\n                            </v-select>\r\n                          </v-flex>\r\n                          <v-flex xs12 sm6 md4>\r\n                            <v-text-field v-model=\"newImage\" label=\"Image URL\"></v-text-field>\r\n                          </v-flex>\r\n                          <v-flex xs12 sm6 md4>\r\n                            <v-text-field v-model=\"newTitle\" label=\"Title\"></v-text-field>\r\n                          </v-flex>\r\n                          <v-flex xs12 sm6 md4>\r\n                            <v-select\r\n                              v-model=\"newCategory\"\r\n                              label=\"Category\"\r\n                              :items=\"categories\"\r\n                              >\r\n                            </v-select>\r\n                          </v-flex>\r\n                          <v-flex xs12 sm6 md4>\r\n                            <v-text-field v-model=\"newQuantity\" label=\"Qty\"></v-text-field>\r\n                          </v-flex>\r\n                          <v-flex xs12 sm6 md4>\r\n                            <v-text-field v-model=\"newCost\" label=\"Cost\"></v-text-field>\r\n                          </v-flex>\r\n                          <v-flex xs12 sm6 md4>\r\n                            <v-text-field v-model=\"newSku\" label=\"SKU\"></v-text-field>\r\n                          </v-flex>\r\n                          <v-flex xs12 sm6 md4>\r\n                            <v-text-field v-model=\"newLocation\" label=\"Location\"></v-text-field>\r\n                          </v-flex>\r\n                          <v-flex xs12 sm6 md4>\r\n                              <v-menu\r\n                                ref=\"menu\"\r\n                                v-model=\"menu\"\r\n                                :close-on-content-click=\"false\"\r\n                                :nudge-right=\"40\"\r\n                                :return-value.sync=\"date\"\r\n                                lazy\r\n                                transition=\"scale-transition\"\r\n                                offset-y\r\n                                full-width\r\n                                min-width=\"290px\"\r\n                              >\r\n                                <template v-slot:activator=\"{ on }\">\r\n                                  <v-text-field\r\n                                    v-model=\"date\"\r\n                                    label=\"Start Date\"\r\n                                    prepend-icon=\"event\"\r\n                                    readonly\r\n                                    v-on=\"on\"\r\n                                  ></v-text-field>\r\n                                </template>\r\n                                <v-date-picker v-model=\"date\" no-title scrollable>\r\n                  